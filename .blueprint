models:
    User: { name: string, email: string, email_verified_at: 'timestamp nullable', password: string, remember_token: 'string:100 nullable' }
    ApproveUser: { id: id, admin_id: 'unsignedBigInteger nullable foreign:users.id', user_id: 'unsignedBigInteger foreign:users.id', expired_at: timestamp, expired: 'boolean default:"0"', status: 'enum:"pending","approved","rejected" default:"pending"', softdeletes: softDeletes }
    Invite: { id: id, token: string, user_id: 'unsignedBigInteger foreign:users.id', email: string, status: 'enum:"pending","approved","rejected" default:"pending"', expired_at: 'timestamp nullable', expired: 'boolean default:"0"', softdeletes: softDeletes }
    Post: { id: id, content: text, platform: 'string nullable', account: 'string nullable', gov_org_id: 'unsignedBigInteger foreign:gov_org.id', softdeletes: softDeletes }
    GovOrg: { id: id, name: string, email: 'string nullable', softdeletes: softDeletes }
    Analyst: { id: id, post_id: 'unsignedBigInteger foreign:posts.id', sentiment: 'enum:"positive","negative","normal" nullable', stance: 'enum:"supportive","against" nullable', softdeletes: softDeletes }
    Hashtag: { id: id, name: string, softdeletes: softDeletes }
    HashtagPost: { id: id, hashtag_id: 'unsignedBigInteger foreign:hashtags.id', post_id: 'unsignedBigInteger foreign:posts.id', softdeletes: softDeletes }
    Rating: { id: id, user_id: 'unsignedBigInteger foreign:users.id', rating: unsignedBigInteger, comment: 'text nullable', softdeletes: softDeletes }
